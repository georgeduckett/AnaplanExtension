{
    "name": "Page Redder",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Turns the page red when you click the icon",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts":
    [
        {
          "matches": ["https://*.app.anaplan.com/*/anaplan/*"],
          "js": ["contentScript.bundle.js"]
        }
    ]
}